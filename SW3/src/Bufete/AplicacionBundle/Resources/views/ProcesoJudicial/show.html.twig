{% extends 'BufeteAplicacionBundle::Default/admin.html.twig' %}

{% block page %}
    <div id="micaso" class="fullsize"  style="  background-color: #F0F0D8;height: 540px;">
    <h1>ProcesoJudicial</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Descripcion</th>
                <td>{{ entity.descripcion }}</td>
            </tr>
            <tr>
                <th>Estado</th>
                <td>{{ entity.estado }}</td>
            </tr>
            <tr>
                <th>Codigoconsulta</th>
                <td>{{ entity.codigoConsulta }}</td>
            </tr>
            <tr>
                <th>Fechain</th>
                <td>{{ entity.fechaIn|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Fechavencimiento</th>
                <td>{{ entity.fechaVencimiento|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Prioridad</th>
                <td>{{ entity.prioridad }}</td>
            </tr>
            
            <tr>
                <th>Abogado a cargo</th>
                <td>{{ entity.abogado }}</td>
            </tr>
            
            <tr>
                <th>Cliente </th>
                <td>{{ entity.cliente }}</td>
            </tr>
            
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('procesojudicial') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('procesojudicial_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
    
    
    </div>
{% endblock %}
